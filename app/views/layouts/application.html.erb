<!DOCTYPE html>
<html>
<head>
  <title>FingerPress</title>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if admin_signed_in? %>
    <%= render 'layouts/navbar' %>
<% else %>
    <%= render 'layouts/navbar_unauthenticated' %>
<% end %>
<div class="container">
  <%= yield %>
</div>
</body>
</html>
