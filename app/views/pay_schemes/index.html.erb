<p id="notice"><%= notice %></p>

<h1>Listing Pay Schemes</h1>

<table>
  <thead>
    <tr>
      <th>Pay type</th>
      <th>Pay</th>
      <th>Pay ot</th>
      <th>Pay public holiday</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pay_schemes.each do |pay_scheme| %>
      <tr>
        <td><%= pay_scheme.pay_type %></td>
        <td><%= pay_scheme.pay %></td>
        <td><%= pay_scheme.pay_ot %></td>
        <td><%= pay_scheme.pay_public_holiday %></td>
        <td><%= link_to 'Show', pay_scheme %></td>
        <td><%= link_to 'Edit', edit_pay_scheme_path(pay_scheme) %></td>
        <td><%= link_to 'Destroy', pay_scheme, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pay scheme', new_pay_scheme_path %>
